            type: "POST",
            url: postUrl,
            dataType: "json",
            success: function (data) {
                $("#" + followed).text("已关注");
                $("#" + followed).removeClass();
                $("#" + followed).addClass("hotbutton unfollow-btn");
                $("#" + followed).attr("follow", true);
            }
